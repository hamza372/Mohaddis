                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="18.5sp"
                    android:textColor="#0e9039"
                    android:textSize="14.8sp"
                    app:fontFamily="@font/urdu_font_family"
                    tools:text="‌صحيح البخاري" />

                <ListView
                    android:id="@+id/listb3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/l4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="8.5dp"
                android:layout_marginRight="13dp"
                android:layout_marginBottom="8.5dp"
                android:background="@drawable/shadow_background"
                android:drawableRight="@drawable/forma_1"
                android:drawablePadding="7dp"
                android:orientation="vertical"
                android:padding="10dp">

                <TextView
                    android:id="@+id/tvb4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/forma_1colorless"
                    android:drawablePadding="7dp"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="18.5sp"
                    android:textColor="#0e9039"
                    android:textSize="14.8sp"
